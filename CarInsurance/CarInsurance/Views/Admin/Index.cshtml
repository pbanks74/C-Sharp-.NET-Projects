@using CarInsurance.Models
@model List<Insuree>

@{ 
    ViewBag.Title = "Admin";
}

<div class="row no-gutters m-3">
  <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
    <div class="card-header text-center">
      <h5 class="m-0">Quotes</h5>
    </div>
    <div class="card-body">
      @foreach (var insuree in Model)
      {
        {
          <input type="hidden" name="Id" value="@insuree.Id" />
          <div class="row mb-3">
            <div class="col-3">
              @insuree.FirstName
            </div>
            <div class="col-3">
              @insuree.LastName
            </div>
            <div class="col-4">
              @insuree.EmailAddress
            </div>
            <div class="col-2">
              @insuree.Quote
            </div>
          </div>
        }
      }
    </div>
  </div>
</div>
